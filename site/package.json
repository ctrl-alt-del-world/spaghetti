    "@sanity/client": "^1.149.7",
    "autoprefixer": "^9.6.0",
    "axios": "^0.19.0",
    "concurrently": "^5.1.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "eleventy-plugin-pwa": "^1.0.8",
    "groq": "^1.149.0",
    "html-minifier": "^4.0.0",
